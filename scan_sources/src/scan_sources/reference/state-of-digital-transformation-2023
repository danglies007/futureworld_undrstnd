<script>

$(document).keydown(function(e) {
      if (e.keyCode == 9){
          $("g", $(".graph-container")).attr("tabIndex", "-1");
          $("a", $(".graph-container")).attr("tabIndex", "-1");
          $("li", $(".graph-container")).attr("tabIndex", "-1");
      }
});

$(window).scroll(function(){
  $("g", $(".graph-container")).attr("tabIndex", "-1");
  $("a", $(".graph-container")).attr("tabIndex", "-1");
  $("li", $(".graph-container")).attr("tabIndex", "-1");
});

$(".donut-graph").each(function() {
    var donutgraph = $(this);   
    
    $(window).scroll(function () {

        var windowHeight = $(window).height();
        var windowScroll = $(window).scrollTop();
                
        var hT = donutgraph.offset().top;
        var hH = donutgraph.outerHeight();
        var showing = donutgraph.attr("data-showing") == "1";

        if (windowScroll + windowHeight > hT && windowScroll < (hT + hH)) {
            if (!showing) {
                donutgraph.attr("data-showing", "1");
                $("svg", donutgraph).hide(1);
                $("svg", donutgraph).show(100);
            }
        }
        else {
            donutgraph.attr("data-showing", "0");
        }
    });
});
</script>
 
